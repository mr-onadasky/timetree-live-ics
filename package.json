{
  "name": "timetree-live-ics",
  "version": "0.8.4",
  "description": "Containerized TimeTree -> ICS exporter with live URL, implemented in TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"ts-node src/index.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "vitest": {
    "exclude": [
      "dist/**",
      "**/node_modules/**"
    ]
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "luxon": "^3.5.0",
    "node-cron": "^4.2.1",
    "pino": "^10.3.0",
    "tsconfig-paths": "^4.2.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@types/node-cron": "^3.0.11",
    "@types/luxon": "^3.4.2",
    "@types/supertest": "^6.0.3",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
